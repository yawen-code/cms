<!-- mybatis-generator的核心配置文件 -->
<generatorConfiguration>

  <!--<classPathEntry location="D:\gg\repository\mysql\mysql-connector-java\8.0.16\mysql-connector-java-8.0.16.jar" />-->
  <classPathEntry location="E:\briup\mysql-connector-java-8.0.11.jar" />
  
 <!--  <classPathEntry location="/Users/lichunyu/springboot/repository/mysql/mysql-connector-java/8.0.16/mysql-connector-java-8.0.16.jar" /> -->
  <context id="DB2Tables" targetRuntime="MyBatis3">

    <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
        connectionURL="jdbc:mysql://127.0.0.1:3306/cms?serverTimezone=UTC&amp;
		useUnicode=true&amp;
		characterEncoding=utf8&amp;
		autoReconnect=true&amp;
		useSSL=false"
        
        userId="briup"
        password="briup">
      <!--产生selectByPrimaryKey-->
      <property name="useInformationSchema" value="true" />
    </jdbcConnection>


	<!--指定生成的类型为java类型，避免数据库中number等类型字段 -->
    <javaTypeResolver >
      <property name="forceBigDecimals" value="false" />
    </javaTypeResolver>

	<!--自动生成的实体的存放包路径 -->
    <javaModelGenerator targetPackage="com.xue.apps.cms.bean" targetProject="./src/main/java">
      <property name="enableSubPackages" value="true" />
      <property name="trimStrings" value="true" />
    </javaModelGenerator>

	<!--自动生成的*Mapper.xml文件存放路径 -->
    <sqlMapGenerator targetPackage="mapper"  targetProject="./src/main/resources">
      <property name="enableSubPackages" value="true" />
    </sqlMapGenerator>

	<!--自动生成的*Mapper.java存放路径 -->
    <javaClientGenerator type="XMLMAPPER" targetPackage="com.xue.apps.cms.dao"  targetProject="./src/main/java">
      <property name="enableSubPackages" value="true" />
    </javaClientGenerator>


